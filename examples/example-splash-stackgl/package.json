{
  "name": "@tstackgl/examples-splash-stackgl",
  "version": "0.0.15",
  "description": "stackgl splash screen",
  "author": "nkint <nootropic.kint@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache build out dist",
    "build": "yarn clean && parcel build src/index.html -d dist --no-cache --detailed-report --public-url ./",
    "start": "parcel src/index.html --open"
  },
  "devDependencies": {
    "parcel-bundler": "^1.10.3",
    "terser": "^3.11.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@thi.ng/hdom": "latest",
    "@thi.ng/hdom-components": "latest",
    "@thi.ng/transducers": "latest",
    "@tstackgl/geometry": "^1.0.10",
    "@tstackgl/regl-draw": "^1.0.12",
    "@tstackgl/types": "^1.0.7",
    "@types/clone": "^0.1.30",
    "bunny": "^1.0.1",
    "clone": "^2.1.2",
    "cube-cube": "^1.0.0",
    "face-normals": "^0.0.0",
    "orbit-camera": "^1.0.0",
    "regl": "^1.3.11",
    "unindex-mesh": "^2.0.0"
  },
  "keywords": [
    "splash",
    "example",
    "webgl",
    "stack.gl",
    "tstack.gl",
    "thi.ng",
    "regl",
    "3d",
    "typescript"
  ],
  "browserslist": [
    "last 3 Chrome versions"
  ],
  "browser": {
    "process": false
  },
  "private": true
}
